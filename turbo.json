{
  "$schema": "https://turbo.build/schema.json",
  "globalDependencies": [
    "**/.env.*local"
  ],
  "pipeline": {
    "build": {
      "dependsOn": [
        "^build"
      ],
      "outputs": [
        ".next/**",
        "!.next/cache/**"
      ],
      "dotEnv": [
        ".env"
      ]
    },
    "lint": {},
    "open:ios": {},
    "open:android": {},
    "sync": {},
    "dev": {
      "cache": false,
      "persistent": true,
      "dotEnv": [
        ".env"
      ]
    }
  }
}